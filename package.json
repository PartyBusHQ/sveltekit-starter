		"algoliasearch": "^4.20.0",
		"bcrypt": "^5.1.1",
		"bits-ui": "^0.21.7",
		"clsx": "^2.1.1",
		"lucia": "^3.2.0",
		"resend": "^3.2.0",
		"svelte-radix": "^1.1.0",
		"svelte-sonner": "^0.3.22",
		"tailwind-merge": "^2.3.0",
		"tailwind-variants": "^0.2.1",
		"uuid": "^9.0.1"
	}
}
